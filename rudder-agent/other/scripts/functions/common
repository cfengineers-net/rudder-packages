function builder() {
	for build in "${OBJECTS[@]}"; do
		cd $SCRIPTDIR
		if [ -f functions/$build.$OS.$ARCH ]; then
			. functions/$build.$OS.$ARCH
		elif [ -f functions/$build.$OS ]; then
			. functions/$build.$OS
		elif [ -f functions/$build.default ]; then
			. functions/$build.default
		else
			echo "ERROR: Unable to locate source file for function build_$build"
			exit 1
		fi
	
		build_$build
	done
}
